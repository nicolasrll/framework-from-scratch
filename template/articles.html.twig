{% extends "template.html.twig" %}
{% block title%}Articles{% endblock %}
{% block content %}
    <h1>Articles</h1>
    <p>{{ message }}</p>
    {% for article in articles %}
        <h2><a href="/article/see?id={{ article.id }}">{{ article.title}}</a></h2>
        <p>
            {{ article.content }}<br>
            <a href="/new?id={{ article.id }}">Modifier</a>
        </p>
    {% endfor %}
{% endblock content %}
